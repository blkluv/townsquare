{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["node"],
    "lib": [
      "es2019",
      "dom"
    ],
    "paths": {
      "lib/*": ["./src/lib/*"],
      "platform/*": ["./src/platform/*"],
      "state/*": ["./src/state/*"],
      "view/*": ["./src/view/*"],
    }
  }
}
